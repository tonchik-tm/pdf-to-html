{
  "name": "tonchik-tm/pdf-to-html",
  "description": "This PHP class can convert your pdf files to html using poppler-utils.",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Nikolaev",
      "email": "tonchik@tonchik.net",
      "homepage": "http://www.tonchik.net",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "pelago/emogrifier": "^5.0.0",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "illuminate/support": "^8.0|^7.0"
  },
  "autoload": {
    "psr-0": {
      "TonchikTm": "src"
    },
    "psr-4": {
      "TonchikTm\\PdfToHtml\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": "TonchikTm\\PdfToHtml\\PdfToHtmlServiceProvider"
    }
  }
}
